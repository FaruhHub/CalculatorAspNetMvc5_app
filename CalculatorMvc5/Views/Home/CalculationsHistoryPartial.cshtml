@model IEnumerable<CalculatorMvc5.Models.CalculationsHistory>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ClientIP)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CalculatedExpression)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CalculatedDatetime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Text)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ClientIP)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CalculatedExpression)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CalculatedDatetime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Text)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
